{
  "buildCommand": "echo 'Static site, no build needed'",
  "devCommand": "npx http-server . -p 3000",
  "installCommand": "echo 'No dependencies to install'",
  "framework": null,
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/management(?:/.*)?",
      "dest": "/management/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    },
    {
      "src": "/((?!api).*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    }
  ]
}
