{
  "routes": [
    {
      "src": "/",
      "has": [
        { "type": "query", "key": "workspace" }
      ],
      "dest": "/management/index.html"
    },
    { "handle": "filesystem" },
    { "src": "/management(?:/.*)?", "dest": "/management/index.html" },
    { "src": "/((?!api).*)", "dest": "/index.html" }
  ]
}
